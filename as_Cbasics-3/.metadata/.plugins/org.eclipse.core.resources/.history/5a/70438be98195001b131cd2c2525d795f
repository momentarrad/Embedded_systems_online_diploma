 */

void main()
{

char operator;
int num1,num2, result;


    switch (operator) {
    case '+':
        result= num1+num2;
        break;
    case '-':
         result= num1-num2;
         break;
    case '*':
    	result= num1*num2;
        break;
    case '/':
    	result= num1/num2;
        break;
        // operator doesn't match any case constant
    default:
        printf("Error! operator is not correct");
    }


    printf(" result is %d", result );
}
