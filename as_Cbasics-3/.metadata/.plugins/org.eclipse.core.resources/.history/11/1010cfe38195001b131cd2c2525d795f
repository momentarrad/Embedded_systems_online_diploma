void main()
{

char operator;
int num1,num2, result;


    switch (operator) {
    case '+':
    result= num1+num2;

        break;
    case '-':
    result= num1-num2;

        break;
    case '*':
        printf("%.1lf * %.1lf = %.1lf", first, second, first * second);
        break;
    case '/':
        printf("%.1lf / %.1lf = %.1lf", first, second, first / second);
        break;
        // operator doesn't match any case constant
    default:
        printf("Error! operator is not correct");
    }

    return 0;
}
