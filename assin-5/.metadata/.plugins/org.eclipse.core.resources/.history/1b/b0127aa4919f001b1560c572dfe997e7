#include <stdio.h>

void main()
{
	int arr[30],no_of_elem,i;
printf("please enter the number of elements");
fflush(stdin);
fflush(stdout);
scanf("%d",&no_of_elem);

printf("please enter the  elements");
fflush(stdin);
fflush(stdout);

for(i=0;i<no_of_elem;i++)
{
	scanf("%d",&arr[i]);
}


}
