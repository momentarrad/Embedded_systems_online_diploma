void find_primeNum_between_2_num(int num1, int num2)
{
	int i=num1+1;
     int j=2;
     char flag=0;


	while(i<num2)
	{
		j=2;
		flag=0;



			while((i/2)>=j)
			{
				if(!(i%j))
						{
					flag=1;
					break;
						}


j++;
		   }
			if(!flag){
				printf("%d is a prime number \n",i);
			         }
		i++;
	}
}






void main()
{
	find_primeNum_between_2_num(0, 10);

}
