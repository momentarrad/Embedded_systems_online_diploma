void find_primeNum_between_2_num(int num1, int num2)
{
	int i=num1+1;
     int j=2;
     char flag=0;


	while(i<num2)
	{
		j=2;



			while((num1/2)>=j)
			{
				if(!(num1%j))
						{
					flag=1;
					break;
						}



		   }
			if(!flag){
				printf("%d is a prime number \n",i);
			         }
		i++;
	}
}

void main()
{
	find_primeNum_between_2_num(1, 10);

}
